{#
 # Available Variables:
 # - items (array)
 #   - title: Link title
 #   - url: Link url
 #   - is_active: (bool) is current page
 #}

<ul>
  {% for item in items %}
    <li>
      {% if not item.is_active %}
          <a href="{{ item.url }}">{{ item.title }}</a>
        {% else %}
          {{ item.title }}
        {% endif %}
    </li>
  {% endfor %}
</ul>
